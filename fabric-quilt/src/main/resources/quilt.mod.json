{
  "schema_version": 1,
  "mixins": [
    "shadowsgate-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "shadowsgate",
    "version": "${version}",
    "metadata": {
      "name": "${displayname}",
      "description": "${description}",
      "contributors": {
        "TheEpicBlock": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/TheEpicBlock/ShadowsGate",
        "issues": "https://github.com/TheEpicBlock/ShadowsGate/issues",
        "sources": "https://github.com/TheEpicBlock/ShadowsGate"
      },
      "license": "MIT",
      "icon": "assets/shadowsgate/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "nl.theepicblock.shadowsgate.fabric.ShadowsGateImpl"
      ]
    },
    "depends": [
      {
        "id": "fabric",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      }
    ]
  },
  "minecraft": {

  }
}